    "ng-packagr": "^2.1.0",
    "ng-zorro-antd": "^0.7.0-beta.2",
    "rxjs": "^5.5.5",
    "sip-lib": "^0.0.2",
    "typescript": "~2.6.0",
    "zone.js": "^0.8.18"
  }
}
