{
  "name": "ng-sip",
  "version": "0.7.0",
  "description": "ng-zorro-antd admin panel front-end framework",
  "author": "sip",
  "homepage": "https://github.com/sip-dev/ng-sip/",
  "bugs": {
    "url": "https://github.com/sip-dev/ng-sip/issues"
  },
  "engines": {
    "node": ">= 8.10.0",
    "npm": ">= 3.0.0"
  },
  "license": "MIT",
  "keywords": [
    "ng-sip",
    "sip-alain",
    "delon",
    "antd",
    "ng-zorro-antd",
    "angular"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration css",
    "start-hmr": "ng serve -c=hmr",
    "start-prod": "ng serve --prod",
    "start-proxy": "ng serve -pc=\"proxy.conf.js\"",
    "build": "ng build --prod --build-optimizer",
    "analyze": "ng build --prod --build-optimizer --stats-json",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "tslint -p src/tsconfig.app.json -c tslint.json 'src/**/*.ts'",
    "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "e2e": "ng e2e",
    "test": "ng test ng-alain --watch=true  --source-map",
    "test-coverage": "ng test --code-coverage --watch=false",
    "copy-sip-alain-css": "cpy ./projects/sip-alain/src/sip/components/sip-table/ng-crud-table/styles/** ./dist/sip-alain/styles/ng-crud-table && cpy ./projects/sip-alain/src/sip/components/sip-table/ng-data-table/styles/** ./dist/sip-alain/styles/ng-data-table && cpy ./projects/sip-alain/src/sip/components/sip-table/styles/** ./dist/sip-alain/styles",
    "copy-sip-alain": "cpy ./** ../../node_modules/sip-alain --cwd=./dist/sip-alain --parents",
    "build-sip-alain-css": "cpy ./projects/sip-alain/src/sip/components/sip-table/ng-crud-table/styles/** ./node_modules/sip-alain/styles/ng-crud-table && cpy ./projects/sip-alain/src/sip/components/sip-table/ng-data-table/styles/** ./node_modules/sip-alain/styles/ng-data-table && cpy ./projects/sip-alain/src/sip/components/sip-table/styles/** ./node_modules/sip-alain/styles",
    "build-sip-alain": "ng build sip-alain --prod && npm run copy-sip-alain-css && npm run copy-sip-alain",
    "publish-sip-alain": "ng build sip-alain --prod && npm run copy-sip-alain-css && npm pu ./dist/sip-alain"
  },
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@delon/abc": "^1.4.1",
    "@delon/acl": "^1.4.1",
    "@delon/auth": "^1.4.1",
    "@delon/cache": "^1.4.1",
    "@delon/form": "^1.4.1",
    "@delon/mock": "^1.4.1",
    "@delon/theme": "^1.4.1",
    "@delon/util": "^1.4.1",
    "@antv/data-set": "^0.8.8",
    "@antv/g2": "^3.0.11",
    "@antv/g2-plugin-slider": "^2.0.2",
    "@ngx-translate/core": "^10.0.1",
    "@ngx-translate/http-loader": "^3.0.1",
    "ajv": "^6.4.0",
    "core-js": "^2.5.4",
    "file-saver": "^1.3.8",
    "ng-zorro-antd": "^1.6.0",
    "ngx-countdown": "^3.0.0",
    "ngx-tinymce": "^2.0.0",
    "ngx-ueditor": "^2.0.0",
    "qrious": "^4.0.2",
    "rxjs": "^6.0.0",
    "screenfull": "^3.3.1",
    "sip-alain": "^0.8.2",
    "sip-lib": "^0.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.1.0",
    "@angular-devkit/build-angular": "~0.7.2",
    "typescript": "2.9.x",
    "@angular/cli": "~6.1.1",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/jszip": "^3.1.3",
    "@types/mockjs": "^1.0.0",
    "codecov": "^3.0.2",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "tslint-language-service": "^0.9.9",
    "editorconfig-tools": "^0.1.1",
    "husky": "^0.14.3",
    "gh-pages": "^1.1.0",
    "lint-staged": "^7.1.0",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^1.12.1",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack-bundle-analyzer": "^2.11.2",
    "xlsx": "^0.12.12",
    "less": "~2.7.0",
    "@angularclass/hmr": "^2.1.3",
    "ng-alain": "^1.1.5",
    "cpy-cli": "^2.0.0",
    "@angular-devkit/build-ng-packagr": "~0.7.2",
    "ng-packagr": "~4.0.1"
  },
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "*.ts": [
      "npm run lint:ts",
      "prettier --write",
      "git add"
    ],
    "src/**/*.less": [
      "npm run lint:style",
      "prettier --write",
      "git add"
    ]
  }
}
