{
  "name": "sip-alain",
  "version": "0.3.0",
  "description": "ng-zorro-antd admin panel front-end framework",
  "author": "sip",
  "homepage": "https://github.com/sip-dev/sip-alain/",
  "bugs": {
    "url": "https://github.com/sip-dev/sip-alain/issues"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  },
  "license": "MIT",
  "scripts": {
    "test": "ng test -sr -sm=false",
    "build": "ng-packagr -p package.json",
    "npmpublish": "npm run build && npm pu ./dist",
    "copytotest":"gulp totest",
    "totest": "npm run build && npm run copytotest"
  },
  "ngPackage": {
    "$schema": "./node_modules/ng-packagr/ng-package.schema.json",
    "lib": {
      "entryFile": "public_api.ts",
      "languageLevel": [
        "dom",
        "es2017"
      ],
      "comments": "none"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@delon/abc": "^1.0.0-beta.4",
    "@delon/acl": "^1.0.0-beta.4",
    "@delon/auth": "^1.0.0-beta.4",
    "@delon/cache": "^1.0.0-beta.4",
    "@delon/mock": "^1.0.0-beta.4",
    "@delon/theme": "^1.0.0-beta.4",
    "@ngx-translate/core": "^9.0.0",
    "@ngx-translate/http-loader": "^2.0.0",
    "@types/node": "~6.0.60",
    "core-js": "^2.5.1",
